<resources>
    <declare-styleable name="ColoredRatingBar">
        <attr name="indicator" format="boolean" />
        <attr name="rating" format="float" />
        <attr name="type" format="enum">
            <enum name="normal" value="0" />
            <enum name="small" value="1" />
        </attr>
        <attr name="coloredRatingBarStyle" format="reference" />
        <!-- Indicator RatingBar style. -->
        <attr name="coloredRatingBarStyleIndicator" format="reference" />
        <!-- Small RatingBar style. -->
        <attr name="coloredRatingBarStyleSmall" format="reference" />
        <!-- Small indicator RatingBar style. -->
        <attr name="coloredRatingBarStyleSmallIndicator" format="reference" />
    </declare-styleable>
    <declare-styleable name="HaisRatingBar">
        <attr name="hindicator" format="boolean" />
        <attr name="hrating" format="float" />
        <attr name="hstartnums" format="integer" />
        <attr name="space" format="integer" />
        <attr name="htype" format="enum">
            <enum name="normal" value="0" />
            <enum name="small" value="1" />
        </attr>
        <attr name="starBackground" format="reference|color" />
        <attr name="starLight" format="reference|color" />
        <attr name="hcoloredRatingBarStyle" format="reference" />
        <!-- Indicator RatingBar style. -->
        <attr name="hcoloredRatingBarStyleIndicator" format="reference" />
        <!-- Small RatingBar style. -->
        <attr name="hcoloredRatingBarStyleSmall" format="reference" />
        <!-- Small indicator RatingBar style. -->
        <attr name="hcoloredRatingBarStyleSmallIndicator" format="reference" />
    </declare-styleable>
</resources><!--


